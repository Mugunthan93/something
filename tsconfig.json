{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext", 
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2015",
      "es2018",
      "es2019",
      "dom",
      "es5",
      "es6",
      "dom.iterable"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "noImplicityAny": true,
    "cognitive-complexity": [true,10],
    "no-big-function":[true,100],
    "strictFunctionTypes": true,
    "noUnusedParameters": true
  }
}